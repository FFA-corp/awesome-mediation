<?xml version="1.0" encoding="utf-8"?><!--    ad_matrix_color_default_cta_bg-->
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/px4" />
            <gradient android:endColor="@color/cta_bg_blue_gradient_end" android:startColor="@color/cta_bg_blue_gradient_start" />
        </shape>
    </item>


    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/px4" />
            <gradient android:endColor="@color/cta_bg_blue_gradient_end_press" android:startColor="@color/cta_bg_blue_gradient_start_press" />
        </shape>
    </item>

</selector>